import java.util.Scanner;

public class project {
    public static void main(String[] args) {
    int heat;
    Scanner sicak=new Scanner(System.in);
        System.out.print("Lutfen Sicaklik Degerini Giriniz :");
        heat=sicak.nextInt();

        if(heat<5){
            System.out.print("Kayak yapabilirsiniz.");
        }
        else if(heat<10){
            System.out.print("Sinemaya gidebilirsiniz.");
        }
        else if(heat>=10 && heat<=15){
            System.out.println("Sinemaya gidebilirsiniz.");
            System.out.println("Piknige gidebilirsinmiz.");
        }
        else if(heat>15&&heat<=25){
            System.out.print("Piknige gidebilirsiniz.");
        }
        else {
            System.out.print("Yuzmeye gidebilirsiniz.");
        }






    }

}
